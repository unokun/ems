DROP TABLE IF EXISTS EMPLOEE;
DROP TABLE IF EXISTS DIVISION;

CREATE TABLE EMPLOEE(ID VARCHAR(10) PRIMARY KEY, NAME VARCHAR(255), AGE INT, SEX VARCHAR(1), PHOTO_ID VARCHAR(10), ADDRESS VARCHAR(256), DIV_ID VARCHAR(10));
CREATE TABLE DIVISION(ID VARCHAR(10) PRIMARY KEY, NAME VARCHAR(255));
CREATE TABLE PHOTO(ID VARCHAR(10) PRIMARY KEY, DATA BLOB);
